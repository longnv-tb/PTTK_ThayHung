<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Login.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doTimServlet</servlet-name>
        <servlet-class>CauHoi.doTimServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doLuuServlet</servlet-name>
        <servlet-class>CauHoi.doLuuServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doNewCauHoiDeThiServlet</servlet-name>
        <servlet-class>CauHoiDeThi.doNewCauHoiDeThiServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>doTaoDeThiServlet</servlet-name>
        <servlet-class>DeThi.doTaoDeThiServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/doLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doTimServlet</servlet-name>
        <url-pattern>/doTim</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doLuuServlet</servlet-name>
        <url-pattern>/doLuu</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doNewCauHoiDeThiServlet</servlet-name>
        <url-pattern>/doNewCauHoiDeThi</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>doTaoDeThiServlet</servlet-name>
        <url-pattern>/doTaoDeThi</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
